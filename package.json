{
  "name": "apilytics",
  "private": true,
  "scripts": {
    "migrate:dev": "prisma migrate dev && prisma generate",
    "migrate:deploy": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate deploy",
    "migrate:reset": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate reset --force",
    "prisma:seed": "prisma db seed",
    "prisma:generate": "prisma generate",
    "dev": "next dev",
    "build": "next build",
    "vercel:build": "if [ \"$VERCEL_ENV\" == 'preview' ]; then yarn migrate:reset; else yarn migrate:deploy; fi && PRISMA_CLIENT_ENGINE_TYPE='dataproxy' yarn prisma:generate && yarn build",
    "start": "next start",
    "format": "prettier --write '**/*.{ts,tsx,js,json,css,prisma,mdx}' && eslint --fix --max-warnings=0 --ext=.ts,.tsx,.js .",
    "lint": "prettier --check '**/*.{ts,tsx,js,json,css,prisma,mdx}' && eslint --max-warnings=0 --ext=.ts,.tsx,.js . && yarn dpdm --tree=false --exit-code=circular:1 $(find src/)",
    "test": "jest --verbose"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@apilytics/next": "1.3.0",
    "@fontsource/ubuntu": "4.5.2",
    "@heroicons/react": "1.0.5",
    "@next-auth/prisma-adapter": "1.0.1",
    "@prisma/client": "3.9.2",
    "aws-sdk": "2.1073.0",
    "clsx": "1.1.1",
    "daisyui": "1.25.4",
    "dayjs": "1.10.7",
    "gray-matter": "4.0.3",
    "next": "12.0.10",
    "next-auth": "4.2.1",
    "next-mdx-remote": "3.0.8",
    "next-plausible": "3.1.4",
    "nodemailer": "6.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "recharts": "2.1.9",
    "rehype-highlight": "5.0.2",
    "remark-heading-id": "1.0.0",
    "simple-encryptor": "4.0.0",
    "slugify": "1.6.5",
    "swagger-ui-dist": "4.5.0",
    "ua-parser-js": "1.0.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "0.5.1",
    "@types/jest": "27.4.0",
    "@types/node": "14.18",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/swagger-ui-dist": "3.30.1",
    "@types/ua-parser-js": "0.7.36",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "autoprefixer": "10.4.2",
    "dpdm": "3.8.0",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "jest": "27.5.1",
    "jest-mock-extended": "2.0.4",
    "node-mocks-http": "1.11.0",
    "postcss": "8.4.6",
    "prettier": "2.5.1",
    "prettier-plugin-prisma": "3.9.0",
    "prisma": "3.9.2",
    "tailwindcss": "3.0.20",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  }
}
