{
  "name": "apilytics",
  "private": true,
  "scripts": {
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset --force",
    "dev": "next dev",
    "build": "BUILD_DATE=\"$(date '+%Y-%m-%d')\" next build",
    "vercel:build": "yarn build && if [ \"$VERCEL_ENV\" == 'preview' ]; then yarn migrate:reset; fi && yarn migrate:deploy",
    "start": "next start",
    "format": "prettier --write '**/*.{ts,tsx,js,json,css,prisma}' && eslint --fix --max-warnings=0 --ext=.ts,.tsx,.js .",
    "lint": "prettier --check '**/*.{ts,tsx,js,json,css,prisma}' && eslint --max-warnings=0 --ext=.ts,.tsx,.js . && yarn dpdm --tree=false --exit-code=circular:1 $(find src/)",
    "test": "jest --verbose"
  },
  "dependencies": {
    "@fontsource/ubuntu": "4.5.0",
    "@heroicons/react": "1.0.5",
    "@next-auth/prisma-adapter": "1.0.1",
    "@prisma/client": "3.6.0",
    "aws-sdk": "2.1046.0",
    "dayjs": "1.10.7",
    "next": "12.0.7",
    "next-auth": "4.0.5",
    "next-plausible": "3.1.4",
    "nodemailer": "6.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "7.1.1",
    "recharts": "2.1.8",
    "simple-encryptor": "4.0.0"
  },
  "devDependencies": {
    "@types/jest": "27.0.3",
    "@types/node": "14.18",
    "@types/react": "17.0.37",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "autoprefixer": "10.4.0",
    "dpdm": "3.8.0",
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "jest": "27.4.5",
    "jest-mock-extended": "2.0.4",
    "node-mocks-http": "1.11.0",
    "postcss": "8.4.5",
    "prettier": "2.5.1",
    "prettier-plugin-prisma": "3.6.0",
    "prisma": "3.6.0",
    "tailwindcss": "3.0.7",
    "typescript": "4.5.4"
  }
}
